  amortisation_pc DECIMAL(17,2) COMMENT 'Amortisation PC',
  control_amort_lc DECIMAL(17,2) COMMENT 'Control_Amort  LC',
  control_amort_pc DECIMAL(17,2) COMMENT 'Control_Amort  PC',
  control_imp_lc DECIMAL(17,2) COMMENT 'Control_Imp LC',
  control_imp_pc DECIMAL(17,2) COMMENT 'Control_Imp PC',
  decrease_lc DECIMAL(17,2) COMMENT 'Decrease LC',
  decrease_pc DECIMAL(17,2) COMMENT 'Decrease PC',
  income_lc DECIMAL(17,2) COMMENT 'Income LC',
  income_pc DECIMAL(17,2) COMMENT 'Income PC',
  impairment_lc DECIMAL(17,2) COMMENT 'Impairment LC',
  impairment_pc DECIMAL(17,2) COMMENT 'Impairment PC',
  increase_lc DECIMAL(17,2) COMMENT 'Increase LC',
  increase_pc DECIMAL(17,2) COMMENT 'Increase PC',
  impairment_stock_lc DECIMAL(17,2) COMMENT 'Impairment stock LC',
  impairment_stock_pc DECIMAL(17,2) COMMENT 'Impairment stock PC',
  reimbursement_lc DECIMAL(17,2) COMMENT 'Reimbursement LC',
  reimbursement_pc DECIMAL(17,2) COMMENT 'Reimbursement PC',
  sales_proceed_lc DECIMAL(17,2) COMMENT 'Sales proceed LC',
  sales_proceed_pc DECIMAL(17,2) COMMENT 'Sales proceed PC',
  wb_amortisation_lc DECIMAL(17,2) COMMENT 'WB Amortisation LC',
  wb_amortisation_pc DECIMAL(17,2) COMMENT 'WB Amortisation PC',
  nbv_sold_lc DECIMAL(17,2) COMMENT 'NBV Sold LC',
  nbv_sold_pc DECIMAL(17,2) COMMENT 'NBV Sold PC',
  wb_imp_flw_reevlt_lc DECIMAL(17,2) COMMENT 'WB Imp flw reevlt LC',
  wb_imp_flw_reevlt_pc DECIMAL(17,2) COMMENT 'WB Imp flw reevlt PC',
  wb_imp_sale_lc DECIMAL(17,2) COMMENT 'WB Imp sale LC',
  wb_imp_sale_pc DECIMAL(17,2) COMMENT 'WB Imp sale PC',
  percentage_of_owner DECIMAL(17,3) COMMENT 'Percentage of owner',
  appraisal_value_lc DECIMAL(17,2) COMMENT 'Appraisal value LC',
  tax_rate DECIMAL(17,3) COMMENT 'Tax rate',
  market_value_val_cur DECIMAL(17,2) COMMENT 'Market value Val Cur')
ROW FORMAT DELIMITED FIELDS TERMINATED BY '|'
LOCATION '/group/pisquare/data/shp_reim';

