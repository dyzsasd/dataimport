  acc_int_stock_pc DECIMAL(17,2) COMMENT 'Acc int  stock PC',
  amort_stock_lc DECIMAL(17,2) COMMENT 'Amort stock LC',
  amort_stock_pc DECIMAL(17,2) COMMENT 'Amort stock PC',
